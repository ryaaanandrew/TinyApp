<h1>TinyApp header partial</h1>


<%if (!username) { %>
    <a href="/login">login</a> <br>
    <a href="/register">Register</a> <br>
<% } else { %>
    <h2>Logged in as: <%= username.email %></h2>
    <form action="/logout" method="POST">
        <button type="submit" class="btn btn-primary">Logout</button>
    </form>

<% } %>














